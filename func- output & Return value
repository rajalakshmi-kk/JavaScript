function calcBottles(startingMoney, CostPerBottle){
     var noofbottles = Math.floor(startingMoney, CostPerBottle);
 return noofbottles;
}
function calChange(startingAmount, CostPerBottle){
    var change = startingAmount % CostPerBottle;
    return change
}
